@PACKAGE_INIT@
set_and_check(TINYSPLINE_INCLUDE_DIRS "@PACKAGE_TINYSPLINE_INSTALL_INCLUDE_DIR@")
set_and_check(TINYSPLINE_LIBRARY_DIRS "@PACKAGE_TINYSPLINE_INSTALL_LIBRARY_DIR@")
set(TINYSPLINE_VERSION "@TINYSPLINE_VERSION@")
set(TINYSPLINE_DEFINITIONS "@TINYSPLINE_DEFINITIONS@")

find_library(TINYSPLINE_LIBRARIES
	@TINYSPLINE_C_LIBRARIES_OUTPUT_NAME@
	HINTS "@PACKAGE_TINYSPLINE_INSTALL_LIBRARY_DIR@"
	NO_DEFAULT_PATH)

mark_as_advanced(
	TINYSPLINE_INCLUDE_DIRS
	TINYSPLINE_LIBRARY_DIRS
	TINYSPLINE_VERSION
	TINYSPLINE_DEFINITIONS
	TINYSPLINE_LIBRARIES)

include(FindPackageHandleStandardArgs)
find_package_handle_standard_args(tinyspline
	REQUIRED_VARS
		TINYSPLINE_INCLUDE_DIRS
		TINYSPLINE_LIBRARY_DIRS
		TINYSPLINE_LIBRARIES
	VERSION_VAR
		TINYSPLINE_VERSION)
